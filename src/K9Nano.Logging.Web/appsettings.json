{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "Sqlite": {
    "LogPath": "logs"
  },
  "Server": {
    "Port": 6253,
    "KeepDays": 30 
  }
}
